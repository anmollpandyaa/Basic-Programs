class Main {
    public static void main(String[] args) {
        int num = 2;
        int flag = 0;
        
        if(num <=1){
            flag = 1;
        }
        for(int i = 2; i <= num/2; i++){
            if(num % i == 0){
                flag = 1;
                break;
            }
        }
        System.out.print(flag == 0 ? "Prime Number" : "Not a Prime Number");
    } 
}
