class Main {
    public static void main(String[] args) {
        int num = 1441;
        int rev = 0;
        int rem;
        int temp = num;
        
        while(num !=0){
            rem = num % 10;
            rev = rev * 10 + rem;
            num /= 10;
        }
        
        System.out.print(temp == rev ? "Palindrome" : "Not a Palindrome");
    } 
}
